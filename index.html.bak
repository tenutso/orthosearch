<head>
    <link href="/css/style.css" rel="stylesheet"></link>

    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap-theme.min.css" integrity="sha384-fLW2N01lMqjakBkx3l/M9EahuwpSfeNvV63J5ezn3uZzapT0u7EYsXMjQV+0En5r" crossorigin="anonymous">
    <link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.css">
    <link href="/css/autocomplete.css" rel="stylesheet"></link>
</head>

<body ng-app="orthosearch" ng-controller="OrthoSearchController as SearchCtrl">
    
        <div id="container">
            <div class="col-md-4">
                <h1>{{SearchCtrl.title}}</h1>
                <p>To find AAO-member orthodontists near you, enter a street address, city, zip/postal code, and/or orthodontist's last name. </p>

                <form id="locatorForm" name="locationForm" >
                    <md-input-container>
                        <input class="form-control" g-places-autocomplete ng-model="SearchCtrl.place" placeholder="Street Address, City, Zip/Postal Code">
                    </md-input-container>
                    <div class="form-group">
                        <!--<input class="form-control" name="location" placeholder="Last Name" id="autocomplete" autocomplete="off" type="text">-->
                        <md-autocomplete 
                        md-items="item in SearchCtrl.getMatches(SearchCtrl.searchText)" 
                        md-item-text="item.display" 
                        md-selected-item="SearchCtrl.selectedItem" 
                        md-search-text="SearchCtrl.searchText" 
                        md-no-cache="true" 
                        placeholder="Last Name">
                            <md-item-template>
                                <span md-highlight-text="ctrl.searchText" md-highlight-flags="^i">{{item.display}}</span>
                            </md-item-template>
                        </md-autocomplete>
                    </div>
                    
                    <div class="form-group">
                        <input class="btn btn-primary" value="Find an Orthodontist" ng-click="SearchCtrl.findByLocation(SearchCtrl)" type="submit">
                    </div>
                </form>
            </div>
        </div>
    
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular.js"></script>
    
    
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular-animate.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular-aria.min.js"></script>
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.4.9/angular-messages.min.js"></script>

  <!-- Angular Material Library -->
  <script src="//ajax.googleapis.com/ajax/libs/angular_material/1.0.0/angular-material.min.js"></script>
    
    
    <script src="/js/autocomplete.js"></script>
    <script src="/js/app.js"></script>
    <script src="/bower_components/ui-bootstrap/src/typeahead/typeahead.js"></script>
    <script src="/bower_components/ui-bootstrap/src/debounce/debounce.js"></script>
    <script src="/bower_components/ui-bootstrap/src/position/position.js"></script>
    
    <script src="//maps.googleapis.com/maps/api/js?key=AIzaSyClJgbgurDmz0Jzojhap-05JkOBB2TgSv8&libraries=places"></script>
</body>